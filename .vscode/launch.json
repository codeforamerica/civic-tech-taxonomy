    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Import CodeForPhilly.org",
            "program": "${workspaceFolder}/import.js",
            "args": [
                "laddr",
                "codeforphilly.org",
                "--commit-to=sites/codeforphilly.org"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import Brigade Matchmaker Dump",
            "program": "${workspaceFolder}/import.js",
            "args": [
                "matchmaker",
                "--commit-to=sites/codeforsanfrancisco.org"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import DemocracyLab",
            "program": "${workspaceFolder}/import.js",
            "args": [
                "democracylab",
                "--commit-to=sites/democracylab.org"
            ]
        }
    ]
}